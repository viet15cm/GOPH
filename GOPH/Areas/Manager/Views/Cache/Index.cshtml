@using GOPH.Areas.Manager.Models;
@model Cache;
@{
    ViewData["Title"] = "Cập nhật bộ nhớ cache";
    var statusMessage = TempData["StatusMessage"] as string;
    Layout = "~/Views/Shared/_LayoutManagerProduct.cshtml";
    ViewData["ActiveNavbar"] = "Cache";
}

<div class="container mt-2">
    <h5><i class="bi bi-arrow-90deg-up"></i> @ViewData["Title"] </h5>
   
    @if (Model?.ObjCache.Count() > 0)
    {
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Tên</th>
                    
                   
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.ObjCache)
                {
                    <tr>
                        <th scope="row">#</th>
                        <td>@item.Value</td>
                    </tr>
                }
            </tbody>
        </table>

        <form method="post" asp-action="update">
            <button type="submit" class="btn btn-sm btn-primary">Cập nhật</button>
        </form>

    }
    else
    {
        @Html.ActionLink("Quay trở lại trang chủ", "Index")
    }

</div>